<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>

        <shape android:shape="oval">

            <solid android:color="#99B5A36A" />
            <size
                android:width="18dp"
                android:height="18dp" />

        </shape>

    </item>

    <item
        android:bottom="3dp"
        android:left="3dp"
        android:right="3dp"
        android:top="3dp">

        <shape android:shape="oval">

            <solid android:color="@android:color/white" />
            <size
                android:width="14dp"
                android:height="14dp" />

        </shape>

    </item>


</layer-list>
